    <script>function pm_install_check() {
  if(!(/* >= */ system.compareVersions(system.version.ProductVersion, '10.6.0') >= 0)) {
    my.result.title = 'Operational system is not supported';
    my.result.message = 'With a great regret we inform you, that your OS version is not supported. Installation is availible for Mac OS X 10.6 and higher.';
    my.result.type = 'Fatal';
    return false;
  }
  return true;
}
</script>
